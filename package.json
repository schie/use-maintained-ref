{
  "name": "use-maintained-ref",
  "version": "1.0.0",
  "description": "creates a ref that is kept up-to-date",
  "author": "schie",
  "license": "MIT",
  "repository": "schie/use-maintained-ref",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "prepare": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "ci:install": "rm -rf ./node_modules && yarn install --frozen-lockfile",
    "ci:shipit": "semantic-release"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^16.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/runtime": "^7.3.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.1",
    "@semantic-release/npm": "^7.1.0",
    "@semantic-release/release-notes-generator": "^9.0.2",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jest": "^23.3.13",
    "@types/react": "^16.7.22",
    "gh-pages": "^2.0.1",
    "prettier": "^2.2.1",
    "jest": "^26.6.3",
    "react": "^16.9.0",
    "react-scripts": "^5.0.0",
    "react-test-renderer": "^16.9.0",
    "semantic-release": "^17.4.2",
    "ts-jest": "^26.5.4",
    "tslib": "^2.0.1",
    "typescript": "^3.2.4"
  }
}
